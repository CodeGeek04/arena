{
    "languages": {
        "python": {
            "extension": ".py",
            "compile": null,
            "execute": [
                "python",
                "/tmp/{filename}"
            ]
        },
        "javascript": {
            "extension": ".js",
            "compile": null,
            "execute": [
                "node",
                "/tmp/{filename}"
            ]
        },
        "rust": {
            "extension": ".rs",
            "compile": [
                "rustc",
                "-O",
                "/tmp/{filename}",
                "-o",
                "/tmp/script"
            ],
            "execute": [
                "/tmp/script"
            ]
        },
        "cpp": {
            "extension": ".cpp",
            "compile": [
                "g++",
                "/tmp/{filename}",
                "-o",
                "/tmp/script"
            ],
            "execute": [
                "/tmp/script"
            ]
        }
    }
}